{
  "name": "@willykit-ui/icons",
  "version": "1.0.1",
  "private": false,
  "license": "MIT",
  "description": "Willykit-UI Design icons distributed as SVG React components.",
  "author": "WUI Team",
  "keywords": [
    "react",
    "react-component",
    "willykit-ui",
    "willykit design",
    "icons"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run --passWithNoTests",
    "test-watch": "vitest",
    "generate-icons": "tsx ./scripts/generate-icons.cli.ts generate ./icons --out-dir src/ --typescript --verbose && npm run lint",
    "generate-index": "tsx scripts/generate-index.ts",
    "build": "tsup",
    "build:prod": "npm run generate-icons && npm run generate-index && npm run build",
    "clean-package": "tsx scripts/clean-package.ts",
    "release": "npm run clean-package && semantic-release",
    "export": "tsx ./scripts/figma-sync.ts"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/commander": "^2.12.5",
    "@types/ejs": "3.1.5",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "commander": "^13.1.0",
    "dotenv": "17.2.3",
    "ejs": "3.1.10",
    "eslint": "^9.21.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "node-fetch": "3.3.2",
    "playwright": "^1.51.1",
    "prettier": "3.5.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "semantic-release": "^24.2.9",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "npm run test"
    ]
  }
}
