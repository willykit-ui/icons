{
  "name": "@willykit-ui/icons",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run --passWithNoTests",
    "test-watch": "vitest",
    "generate-icons": "tsx scripts/generate-icons.ts ./src/assets/icons --out-dir src/components --typescript && prettier --write src/components/**/*.tsx",
    "generate-index": "tsc && node dist/scripts/generate-index.js",
    "build:prod": "npm run generate-icons && npm run generate-index && tsup --dts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "semantic-release"
  },
  "author": "WUI Team",
  "license": "MIT",
  "description": "Willykit-UI Design icons distributed as SVG React components.",
  "keywords": [
    "react",
    "react-component",
    "willykit-ui",
    "willykit design",
    "icons"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "dependencies": {
    "globals": "^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-onboarding": "^8.6.7",
    "@storybook/blocks": "^8.6.7",
    "@storybook/builder-vite": "^8.6.7",
    "@storybook/experimental-addon-test": "^8.6.7",
    "@storybook/react": "^8.6.7",
    "@storybook/react-vite": "^8.6.7",
    "@storybook/test": "^8.6.7",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/commander": "^2.12.5",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "commander": "^13.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "storybook": "^8.6.7",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.9"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "peerDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "npm run test"
    ]
  }
}
